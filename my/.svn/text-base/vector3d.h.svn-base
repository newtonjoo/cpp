
class vector3d
{
 private:
   double x, y, z;
 public:
   vector3d(double ax, double ay, double az) {
      x= ax; y= ay, z= az;
   };
   ~vector3d() {};
   void out();
   double length();
   double dotproduct(vector3d vec);
   vector3d crossproduct(vector3d vec);
   void translation(double a, double b, double c);
   void rotation(double alpha, double beta, double gamma);
};
